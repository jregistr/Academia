<Pane maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Suggestron.Suggestor">
    <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="57.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#cdcdcd" height="346.0" layoutY="57.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
        <Label alignment="CENTER" layoutX="-8.0" maxWidth="616.0" prefHeight="57.0" prefWidth="616.0" text="Welcome to Suggestron 9000!!" textAlignment="CENTER" textFill="black">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
      <TextField fx:id="userURL" disable="true" layoutX="6.0" layoutY="79.0" onMouseClicked="#urlBoxClicked" opacity="0.0" prefHeight="57.0" prefWidth="441.0" promptText="Enter a valid url" />
      <Button fx:id="userURLButton" disable="true" layoutX="452.0" layoutY="79.0" mnemonicParsing="false" onMouseClicked="#urlButtonClicked" opacity="0.0" prefHeight="57.0" prefWidth="142.0" text="Button" />
      <Hyperlink fx:id="suggestionHyper" alignment="CENTER" disable="true" layoutX="3.0" layoutY="218.0" onMouseClicked="#matchedHyperClicked" opacity="0.0" prefHeight="45.0" prefWidth="594.0" text="Hyperlink">
         <font>
            <Font name="Arial Bold Italic" size="20.0" />
         </font>
      </Hyperlink>
      <ImageView fx:id="refreshingImage" disable="true" fitHeight="270.0" fitWidth="270.0" layoutX="149.0" layoutY="89.0" opacity="0.0">
         <image>
            <Image url="@refresh.png" />
         </image>
      </ImageView>
      <Button fx:id="initButton" layoutX="13.0" layoutY="71.0" mnemonicParsing="false" onMouseClicked="#initButtonClicked" prefHeight="307.0" prefWidth="575.0" text="Initialise">
         <font>
            <Font size="64.0" />
         </font>
      </Button>
        
    </children>
</Pane>
