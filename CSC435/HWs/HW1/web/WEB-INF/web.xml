<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>IntroServlet</servlet-name>
        <servlet-class>FleetGame.IntroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>FleetGame.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShipInformations</servlet-name>
        <servlet-class>FleetGame.ShipInformations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BattleHistory</servlet-name>
        <servlet-class>FleetGame.BattleHistory</servlet-class>
    </servlet>

    <welcome-file-list>
        <welcome-file>Intro</welcome-file>
    </welcome-file-list>

    <servlet-mapping>
        <servlet-name>IntroServlet</servlet-name>
        <url-pattern>/Intro</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ShipInformations</servlet-name>
        <url-pattern>/ShipInfo</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>BattleHistory</servlet-name>
        <url-pattern>/History</url-pattern>
    </servlet-mapping>

    
</web-app>